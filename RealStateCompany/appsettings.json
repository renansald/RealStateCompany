{
  "AppSettings": {
    "TokenKey": "my token key"
  },
  "ConnectionStrings": {
    "Default": "Host=host;Database=database;user=user;password=password"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "application insights key"
  },
  "Logging": {
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Error"
      }
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
